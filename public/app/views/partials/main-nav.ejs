<nav class="main-nav no-active collapse">
    <div class="header-nav">
        <a href="/" class="logo">
            <img src="/resources/icons/favicon.ico" alt="Logo" />
        </a>
        <div class="action-nav">
            <i class="icon icon-hamburger"></i>
        </div>
    </div>
    <div class="main-nav-menus">
        <div class="header-mobile-vers">
            <ul class="header-buttons">
                <% headerButtons.forEach(item=> { %>
                    <li>
                        <div class="icon <%= item.icon %>"></div>
                    </li>
                <% }) %>

                <li class="profile-section">
                    <img src="<%= avatarSrc %>" id="mobile-<%= avatarId %>">
                </li>
            </ul>
            <button type="button" class="icon-button button-active" btn-id="close">
                <i class="icon icon-close"></i>
            </button>
        </div>
        <ul class="menu-nav">
            <% menuItems.forEach(item=> { %>
                <li>
                    <a href="<%= item.href %>" class="<%= item.label===activeLabel ? 'active' : '' %>">
                        <span>
                            <%= item.label %>
                        </span>
                        <i class="icon <%= item.icon %>"></i>
                    </a>
                </li>
                <% }) %>
        </ul>
    </div>

</nav>